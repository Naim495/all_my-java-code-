import java.util.Scanner;

class Main {
    public static void main(String[] args) {
         System.out.println("even numbers are :  ");
        
        int i = 1; 
        
        while (i <= 50) {
            i++;
            if (i % 2 == 0) {
                System.out.println(i); 
            } else {
                System.out.print(" ");
            }
        }
    }
}
